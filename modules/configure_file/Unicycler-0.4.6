module-whatis   "loads the [module-info name] environment"




proc ModulesHelp { } {
   puts stderr "\tSee How_to_use file: /usr/local/bioinfo/src/Unicycler/How_to_use_SLURM_Unicycler"
}


prereq compiler/gcc-4.9.1 bioinfo/SPAdes-3.11.1 bioinfo/racon-v1.3.1 bioinfo/pilon-v1.22 bioinfo/bowtie2-2.3.3.1 bioinfo/samtools-1.8 bioinfo/ncbi-blast-2.6.0+

if { [ module-info mode load ] } {
  puts stdout ". /usr/local/bioinfo/src/Unicycler/Unicycler-v0.4.6_env/bin/activate;"
} elseif { [ module-info mode remove ] } {
  puts stdout "deactivate;"
}

