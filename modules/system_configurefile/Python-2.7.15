proc ModulesHelp { } {
    global name version prefix man_path

    puts stderr "\t[module-info name] - loads the $name environment"
    puts stderr "\tThe following env variables are set:"
    puts stderr "\t\t\PATH"
}



module-whatis   "loads the [module-info name] environment"

prepend-path    PATH    /tools/python/2.7.15/bin

