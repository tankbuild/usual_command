module-whatis   "loads the [module-info name] environment"




proc ModulesHelp { } {
   puts stderr "\tSee How_to_use file: /usr/local/bioinfo/src/Cogent/How_to_use_SLURM_Cogent"
}



prereq system/Miniconda3-4.4.10

prepend-path LD_LIBRARY_PATH /usr/local/bioinfo/src/Cogent/Cogent-v3.6/Complete-Striped-Smith-Waterman-Library/src/
prepend-path PYTHONPATH /usr/local/bioinfo/src/Cogent/Cogent-v3.6/Complete-Striped-Smith-Waterman-Library/src/

if { [ module-info mode load ] } {
  puts stdout "source activate cogent-v3.6_env;"
} elseif { [ module-info mode remove ] } {
  puts stdout "source deactivate;"
}
